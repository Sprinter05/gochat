    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "args": [
                "../.env",
                "../logs/sql.log"
            ],
            "program": "${workspaceFolder}/server",
        },
        {
            "name": "Server Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/server/test",
            "preLaunchTask": "Clean_Test_Cache"
        }
    ]
}